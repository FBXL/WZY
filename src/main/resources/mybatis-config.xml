<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>


    <settings>
        <setting name="logImpl" value="STDOUT_LOGGING"/>
    </settings>

    <!--配置包的别名-->
    <typeAliases>
        <package name="com.wzy.pojo"/>
    </typeAliases>

    <!--绑定-->
    <mappers>
        <!--dao和mapper文件名字一样就用class    名字不一样用resource-->
        <mapper class="com.wzy.dao.BookMapper"></mapper>
        <mapper class="com.wzy.dao.AdminMapper"></mapper>
    </mappers>



</configuration>